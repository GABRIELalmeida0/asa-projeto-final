;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	portal.asa.br. root.portal.asa.br. (
			    350		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN  MX 10 mail.asa.br.
@	IN	NS	ns1.asa.br.

ns1			A	192.168.0.115
mail		A 	192.168.0.115
www			CNAME	ns1
